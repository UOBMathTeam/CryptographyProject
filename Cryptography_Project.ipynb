from time import process_time   


def generate_N(k,n):
    alist = ([])
    for i in range(2*n):
        alist.append(random_prime(k,40))
    x = np.array_split(alist,n)
    blist = ([])
    for j in range(n):
        blist.append(np.product(x[j]))
    return blist

